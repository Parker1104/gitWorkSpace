  	<select id="selectReserved" resultMap="BaseResultMap"
		parameterType="com.hzdongcheng.components.toolkits.utils.Page">
		select * from (select userCardID, IFNULL(idCode,'') as idCode,
		IFNULL(userName, '')
		as userName,'' as makeDate, '' as endDate, '' as
		displayName, '' as
		boxID , 0 as binding from `user`
		UNION ALL select
		userCardID, IFNULL(idCode,'') as idCode,
		IFNULL(userName,'') as
		userName, storeInTime,
		IFNULL(effectiveDays,'')
		as effectiveDays,
		terminalID, boxID ,
		1 as binding
		FROM storeinrecord where
		state = 0
		) as a 
		WHERE 1 = 1
		<trim suffixOverrides=",">
			<include refid="queryReserved" />
		</trim>
		order by makeDate desc
		limit
		#{startIndex,jdbcType=INTEGER},#{pageSize,jdbcType=INTEGER}
	</select>
	<select id="getReservedCount" parameterType="com.hzdongcheng.components.toolkits.utils.Page"
		resultType="java.lang.Integer">
		select count(*) from (select userCardID, IFNULL(idCode,'')
		as idCode,
		IFNULL(userName, '')
		as userName,'' as makeDate, '' as
		endDate, '' as displayName, '' as
		boxID , 0 as binding from `user`
		UNION ALL select userCardID, IFNULL(idCode,'') as idCode,
		IFNULL(userName,'') as userName, storeInTime,
		IFNULL(effectiveDays,'')
		as effectiveDays, terminalID , boxID ,
		1 as binding
		FROM storeinrecord where state = 0) as a
		order by makeDate desc
		limit
		#{startIndex,jdbcType=INTEGER},#{pageSize,jdbcType=INTEGER}
	</select>
  
  
